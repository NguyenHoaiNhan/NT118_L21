<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@color/almond"
  android:orientation="vertical">
  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="20dp"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/employee_greeting"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_marginStart="25dp"
        android:layout_weight="0.9"
        android:fontFamily="@font/futuramedium"
        android:text="@string/healthgolable"
        android:textColor="@color/black"
        android:textSize="36sp" />

    <ImageView
      android:id="@+id/employee_logout"
      android:layout_width="35dp"
      android:layout_height="35dp"
      android:layout_gravity="end"
      android:layout_marginTop="20dp"
      android:layout_marginEnd="30dp"
      android:clickable="true"
      app:srcCompat="@drawable/logout" />
  </LinearLayout>


  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="10dp"
    android:layout_marginBottom="-15dp"
    android:background="@drawable/rounded_40dp"
    android:backgroundTint="@color/white"
    android:orientation="vertical">

    <LinearLayout
      android:id="@+id/linear_gym1"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:clickable="true"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginRight="7.5dp"
          android:layout_marginBottom="7.5dp"
        />

      </LinearLayout>

      <TextView
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="YOGA"
          android:textAlignment="viewStart"
          android:textColor="@color/black"
          android:textSize="20sp" />

    </LinearLayout>

    <View
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="center"
      android:layout_marginStart="35dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="45dp"
      android:layout_marginBottom="5dp"
      android:background="@color/light_grey"
      android:baselineAligned="false" />

    <LinearLayout
      android:id="@+id/linear_gym2"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:clickable="true"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:id="@+id/imageView3"
          android:layout_width="47dp"
          android:layout_height="40dp"
          android:layout_marginLeft="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginRight="7.5dp"
          android:layout_marginBottom="7.5dp"
           />

      </LinearLayout>

      <TextView
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="Cơ ngực"
          android:textAlignment="viewStart"
          android:textColor="@color/black"
          android:textSize="20sp" />

    </LinearLayout>

    <View
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="center"
      android:layout_marginStart="35dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="45dp"
      android:layout_marginBottom="5dp"
      android:background="@color/light_grey"
      android:baselineAligned="false" />

    <LinearLayout
      android:id="@+id/linear_gym3"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:clickable="true"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:layout_width="47dp"
          android:layout_height="40dp"
          android:layout_marginLeft="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginRight="7.5dp"
          android:layout_marginBottom="7.5dp"
          />

      </LinearLayout>

      <TextView
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="Cơ bụng"
          android:textAlignment="viewStart"
          android:textColor="@color/black"
          android:textSize="20sp" />

    </LinearLayout>

    <View
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="center"
      android:layout_marginStart="35dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="45dp"
      android:layout_marginBottom="5dp"
      android:background="@color/light_grey"
      android:baselineAligned="false" />

    <LinearLayout
      android:id="@+id/linear_gym4"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:clickable="true"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:layout_width="47dp"
          android:layout_height="40dp"
          android:layout_marginLeft="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginRight="7.5dp"
          android:layout_marginBottom="7.5dp"
          app:srcCompat="@drawable/membership" />

      </LinearLayout>

      <TextView
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="Cơ mông"
          android:textAlignment="viewStart"
          android:textColor="@color/black"
          android:textSize="20sp" />

    </LinearLayout>

    <View
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="center"
      android:layout_marginStart="35dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="45dp"
      android:layout_marginBottom="5dp"
      android:background="@color/light_grey"
      android:baselineAligned="false" />

    <LinearLayout
      android:id="@+id/linear_gym5"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:clickable="true"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:id="@+id/imageView2"
          android:layout_width="45dp"
          android:layout_height="45dp"
          android:layout_marginLeft="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginRight="10dp"
          android:layout_marginBottom="7.5dp"
          android:baselineAlignBottom="false"
          android:textAlignment="center"
           />
      </LinearLayout>

      <TextView
          android:id="@+id/textView5"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="Tìm kiếm trên Internet"
          android:textAlignment="viewStart"
          android:textColor="#000000"
          android:textSize="20sp" />

    </LinearLayout>

    <View
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="center"
      android:layout_marginStart="35dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="45dp"
      android:layout_marginBottom="5dp"
      android:background="@color/light_grey"
      android:baselineAligned="false" />

    <LinearLayout
      android:id="@+id/linear_gym6"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:id="@+id/imageView"
          android:layout_width="45dp"
          android:layout_height="45dp"
          android:layout_marginLeft="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginRight="10dp"
          android:layout_marginBottom="7.5dp"
          />

      </LinearLayout>

      <TextView
          android:id="@+id/add_item_itemname"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="Bấm giờ"
          android:textColor="@color/black"
          android:textSize="20sp" />

    </LinearLayout>

    <View
      android:layout_width="match_parent"
      android:layout_height="2dp"
      android:layout_gravity="center"
      android:layout_marginStart="35dp"
      android:layout_marginTop="5dp"
      android:layout_marginEnd="45dp"
      android:layout_marginBottom="5dp"
      android:background="@color/light_grey"
      android:baselineAligned="false" />

    <LinearLayout
      android:id="@+id/linear_gym7"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_marginBottom="25dp"
      android:layout_weight="0.1"
      android:clickable="true"
      android:orientation="horizontal">

      <LinearLayout
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/rounded_20dp"
        android:backgroundTint="#ff8140"
        android:orientation="vertical">

        <ImageView
          android:id="@+id/insert_image"
          android:layout_width="45dp"
          android:layout_height="45dp"
          android:layout_marginStart="7.5dp"
          android:layout_marginTop="7.5dp"
          android:layout_marginEnd="10dp"
          android:layout_marginBottom="7.5dp"
          app:srcCompat="@drawable/insert_item" />
      </LinearLayout>

      <TextView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="25dp"
          android:layout_marginBottom="20dp"
          android:fontFamily="@font/futuramedium"
          android:text="Lịch sử"
          android:textColor="@color/black"
          android:textSize="20sp" />
    </LinearLayout>
  </LinearLayout>
</LinearLayout>